{
    "name": "huangdijia/laravel-ssdb",
    "description": "ssdb for laravel",
    "type": "library",
    "keywords": [
        "laravel",
        "ssdb"
    ],
    "homepage": "https://github.com/huangdijia/laravel-ssdb",
    "license": "MIT",
    "authors": [{
        "name": "huangdijia",
        "email": "huangdijia@gmail.com"
    }],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/contracts": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Huangdijia\\Ssdb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Huangdijia\\Ssdb\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Huangdijia\\Ssdb\\SsdbServiceProvider"
            ],
            "aliases": {
                "Ssdb": "Huangdijia\\Ssdb\\Facades\\Ssdb"
            }
        }
    }
}